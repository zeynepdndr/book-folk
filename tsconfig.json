  "compilerOptions": {
    "allowJs": true,
    "jsx": "react-jsx",
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5"
  },
  "include": ["./src/**/*"],
  "exclude": ["node_nodules"]
}
